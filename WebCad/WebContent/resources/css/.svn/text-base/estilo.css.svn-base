@CHARSET "ISO-8859-1";

.conteudo{
	position:relative;
	background-color: white;
	border-bottom: solid 1px #D2D3D5;
	border-left: solid 1px #D2D3D5;
	border-right: solid 1px #D2D3D5;
	border-radius: 0 0 10px 10px;
	padding: 10px;
	margin-left: 10px;
	margin-right: 10px;
}

.login{
	position:relative;
	margin: 0 auto;
	width: 270px;
}

.form_login{
	position: relative;
	margin: 0 auto;
	width: 260px;
	padding-bottom: 20px;
}

.form{
	position: relative;
	margin: 0 auto;
	width: 260px;
	padding-bottom: 20px;
	float: left;
}

.link-atividades-principais-home{
	position: relative;
	float: left;
	background-color: white;
	display: block;
	float: left;
	height: 95px;
	width: 95px;
	color: #565656;
	border: 1px solid #CCC;
	border-radius: 5px;
	margin-right: 15px;
	margin-bottom: 15px;
	font-size: 10px;
	text-decoration: none;
}
.link-atividades-principais-home:hover{
	background-color: #FAFAFA;
	box-shadow:0px 0px 10px 5px #F0F0F0;
}
.link-atividades-principais-home img{
	position:relative;
	padding: 5px 0;
	margin-left: 20px;
}
.link-atividades-principais-home span{
	position:relative;
	display: block;
	text-align: center;
}
.menu {
	list-style:none;
	float:left;
	z-index: 5000;
	margin-top:-10px;
	margin-left: 0;
}
.menu li {
	position:relative;
	float:left;
	z-index: 5000;
	border: none;
	background-color:#F1F1F2;
}
.menu li a {
	font-size:10px;
	font-weight:bold;
	color:black;
	text-decoration:none;
	text-align:center;
	padding:6px 6px;
	z-index: 5000;
	display:block;
	border: none;
	border-right: solid 1px white;
}
.menu li a:hover {
	background-color: #E0E1E0;
}
.menu li ul li:hover{
	background-color: #E0E1E0;
}
.menu li ul li a{
	font-weight: normal;
	text-align: left;
	padding: 0px;
	border-bottom: solid 1px #D8D8D8;
	width: 100%;
	background-color: black;
}
.menu li ul li a span{
	position:relative;
	float:left;
	font-size:10px;
	padding: 5px;
	margin: 0;
	width: 74%;
}
.menu li ul li a img{
	position:relative;
	float:left;
	background-color:#D8D8D8;
	margin: 0;
	padding: 3px;
	width: 17px;
}
.menu li ul {
	position:absolute;
	top:24px;
	left:-5px;
	z-index: 5000;
	display:none;
	border: none;
	width: 150px;
}
.menu li:hover ul, .menu li ul li:hover ul, .menu li.over ul, .menu li ul li.over ul {
	display:block;
	z-index: 5000;
	border: none;
}
.menu li ul li {
	display:block;
	border: none;
	width:170px;
	left:-20px;
	z-index: 5000;
}
.menu li ul li ul {
	z-index:900;
	top:0;
	left:140px;
	width:100px;
	border: none;
	display: none;
}
.menu li ul li:hover  ul li{
	display:block;
	z-index: 5000;
}
.menu li ul li ul li{
	display: none;
	position: relative;
	margin-left: -20px;
}
.left_home{
	position:relative;
	

	
}
.left_home_adm{
	float: left;
	width: 30%;
	margin-top: 10px;

	
}
.right_home{
	float: right;
	width: 65%;
	margin-top: 10px;
}